{
  "name": "mao-rn-android-kit",
  "version": "0.4.0",
  "author": "maolion.j@gmail.com",
  "description": "为 React Native 开发 提供的一些Android原生模块/组件 (react native, CoordinatorLayout, AppBarLayout, TabLayout, NestedScrollView, PopupWindow)",
  "keywords": [
    "mao-rn-android-kit",
    "react-native",
    "react native",
    "android",
    "support design",
    "AppBarLayout",
    "NestedScrollView",
    "CoordinatorLayout",
    "PopupWindowAndroid",
    "NestedScrollView",
    "PopupWindow",
    "Gravity",
    "Dimensions"
  ],
  "bugs": {
    "url": "https://github.com/maolion/mao-rn-android-kit/issues"
  },
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maolion/mao-rn-android-kit.git"
  },
  "homepage": "https://github.com/maolion/mao-rn-android-kit",
  "scripts": {
    "start": "tsc -v && node -harmony devtools/debug.js",
    "build": "tsc -v && node -harmony devtools/build.js",
    "build-local": "npm run build && rimraf ./example/node_modules/mao-rn-android-kit && mkdir ./example/node_modules/mao-rn-android-kit && cp -r ./dist ./example/node_modules/mao-rn-android-kit && cp package.json ./example/node_modules/mao-rn-android-kit",
    "prepublish": "rimraf dist && npm run build",
    "deploy": "npm publish"
  },
  "dependencies": {
    "react-mixins-decorator": "^0.1.10",
    "thenfail": "^0.4.12",
    "x-dts-generator": "^1.0.6"
  },
  "devDependencies": {
    "chokidar": "^1.6.1",
    "fs-extra": "^1.0.0",
    "rimraf": "^2.5.4",
    "typescript": "^2.1.6",
    "x-dts-generator": "^1.0.0"
  }
}
