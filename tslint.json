{
  "extends": "vts",
  "rules": {
    "trailing-comma": [ false ],
    "no-console": [false],
    "no-empty-interface": false,
    "prefer-template": false,
    "prefer-method-signature": false,
    "variable-name": [
      true,
      "ban-keywords",
      "allow-pascal-case",
      "allow-trailing-underscore",
      "allow-leading-underscore"
    ]
  }
}
